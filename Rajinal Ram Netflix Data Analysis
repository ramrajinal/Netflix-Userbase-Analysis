   ],
   "source": [
    "z = df.groupby(['View Rating']).size().reset_index(name='counts')\n",
    "explode = []\n",
    "for i in z['counts']:\n",
    "    value = 0.0\n",
    "    if i < 100:\n",
    "        value = 1.0\n",
    "    explode.append(value)\n",
    "print(explode)\n",
    "\n",
    "pieChart = plt.pie(z['counts'], radius=2.0, autopct='%1.1f%%', explode=explode)\n",
    "\n",
    "plt.legend(labels=z['View Rating'], bbox_to_anchor=(2.5, 1.4))"
   ]
