   "source": [
    "#### Drop IMDb Votes < 100000"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 118,
   "id": "3e863e2d",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "  View Rating  Total IMDb Votes\n",
      "0           R       197061884.0\n",
      "1       PG-13       150149460.0\n",
      "2          PG        42428701.0\n",
      "3       TV-MA        13321357.0\n",
      "4       TV-14        11145627.0\n",
      "5       TV-PG         4023591.0\n",
      "6           G         3830307.0\n",
      "7   Not Rated         3007867.0\n",
      "8    Approved          981521.0\n",
      "9      Passed          709677.0\n"
     ]
    }
   ],
   "source": [
    "cdf = cdf.dropna(subset=['View Rating'])\n",
    "cdf = cdf.drop(cdf[(cdf['IMDb Votes'] < 100000)].index)\n",
    "\n",
    "z = cdf.groupby('View Rating')['IMDb Votes'].sum().sort_values(\n",
    "    ascending=False).reset_index(name='Total IMDb Votes')\n",
    "z = z.drop(z[(z['Total IMDb Votes'] < 500000)].index)\n",
    "print(z)\n",
    "#240Million"
   ]
  },
